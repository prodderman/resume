{
  "name": "webpack-2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "prod": "better-npm-run build-prod",
    "dev": "better-npm-run build-dev"
  },
  "betterScripts": {
    "build-prod": {
      "command": "./node_modules/.bin/webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-dev": {
      "command": "./node_modules/.bin/webpack-dev-server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 8080
      }
    }
  },
  "author": "Stepan",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^3.11.0",
    "webpack-config": "^6.1.1",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "bundle-loader": "^0.5.6",
    "clean-webpack-plugin": "^0.1.18",
    "css-loader": "^0.28.10",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "html-webpack-harddisk-plugin": "^0.1.0",
    "html-webpack-plugin": "^2.30.1",
    "jquery": "^3.2.1",
    "node-static": "^0.7.9",
    "normalize.css": "^7.0.0",
    "pug": "^2.0.0-rc.3",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  }
}
