{
  "name": "webpack-2",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "watch": "better-npm-run build-watch",
    "prod": "better-npm-run build-prod",
    "dev": "better-npm-run build-dev"
  },
  "betterScripts": {
    "build-prod": {
      "command": "webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "prod"
      }
    },
    "build-watch": {
      "command": "webpack",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev"
      }
    },
    "build-dev": {
      "command": "webpack-dev-server",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "dev",
        "PORT": 8888
      }
    }
  },
  "author": "Stepan",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "better-npm-run": "0.0.15",
    "css-loader": "^0.28.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "html-webpack-plugin": "^2.28.0",
    "node-static": "^0.7.9",
    "pug": "^2.0.0-rc.2",
    "pug-loader": "^2.3.0",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "webpack": "^3.3.0",
    "webpack-config": "^7.0.0",
    "webpack-dev-server": "^2.6.1"
  },
  "dependencies": {
    "glob": "^7.1.2"
  }
}
